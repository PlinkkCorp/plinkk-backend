{
  "scripts": {
    "prisma:migratedev": "prisma migrate dev",
    "prisma:dev": "prisma dev",
    "prisma:studio": "prisma studio",
    "prisma:init": "prisma migrate deploy && prisma generate --schema=../../prisma/schema.prisma",
    "postinstall": "prisma generate --schema=packages/prisma/prisma/schema.prisma || echo prisma-generate-skipped",
    "set-admin": "node apps/dashboard/scripts/set-admin.cjs",
    "migrate:plinkks": "node scripts/migrate-plinkks.cjs",
    "delete_inactive": "node scripts/delete_inactive_user.js ",
    "seed:role": "ts-node prisma/seed.ts",
    "prisma:generate": "prisma generate --generator=dashboard && prisma generate --generator=public",
    "build": "tsc -p tsconfig.json || true && node scripts/copy-static.mjs",
    "launch-all": "concurrently \"pnpm --filter @plinkk/dashboard dev\" \"pnpm --filter @plinkk/public dev\""
  },
  "devDependencies": {
    "prisma": "^7.0.0"
  },
  "dependencies": {
    "@fastify/secure-session": "^8.2.0",
    "@prisma/client": "^7.0.0",
    "dotenv": "^17.2.3"
  },
  "private": true
}
