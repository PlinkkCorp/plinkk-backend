<!DOCTYPE html>
<html lang="fr">

<head>
    <link rel="stylesheet" href="/css/styles.css">
    <link rel="stylesheet" href="/css/button.css">
    <link rel="icon" href="https://s3.marvideo.fr/plinkk-image/logo.svg" />
    <meta name="title" content="<%= (page && page.title) ? page.title : username %>">
    <meta name="description"
        content="<%= (page && page.description) ? page.description : 'Découvre mon profil sur Plinkk !' %>">
    <meta name="keywords"
        content="Plinkk, Klaynight Dev, Profil, Secondaire, Page, Linktree, Bio, Social, Réseaux sociaux, Portfolio, CV, Liens, Partage, Personnalisation, Thèmes, Cosmétiques">
    <meta property="og:type" content="profile">
    <meta property="og:url"
        content="<%= (typeof publicPath !== 'undefined' && publicPath) ? publicPath : ((page && page.slug) ? page.slug : username) %>">
    <meta property="og:title" content="<%= (page && page.title) ? page.title : username %> sur Plinkk">
    <meta property="og:description"
        content="<%= (page && page.description) ? page.description : 'Découvre mon profil sur Plinkk !' %>">
    <meta property="og:image" content="/og/<%= (page && page.title) ? page.title : username %>">
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url"
        content="<%= (typeof publicPath !== 'undefined' && publicPath) ? publicPath : ((page && page.slug) ? page.slug : username) %>">
    <meta property="twitter:title" content="<%= (page && page.title) ? page.title : username %> sur Plinkk">
    <meta property="twitter:description"
        content="<%= (page && page.description) ? page.description : 'Découvre mon profil sur Plinkk !' %>">
    <meta property="twitter:image" content="/api/plinkks/og/<%= (page && page.title) ? page.title : username %>">
    <meta property="twitter:image:alt" content="Plinkk">
    <meta name="theme-color" content="#7289DA">
    <meta name="author" content="Plinkk">
    <meta name="robots" content="index, follow">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8">
    <script defer src="/umami_script.js" data-website-id="69e21905-d1fc-48c8-a64e-26a6ad5ad089"
        data-host-url="https://analytics.plinkk.fr"></script>
    <meta http-equiv="Permissions-Policy" content="unload=*">
    <meta http-equiv="Content-Security-Policy" content="default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://unpkg.com https://cdnjs.cloudflare.com https://cdn.jsdelivr.net https://plinkk.fr https://analytics.plinkk.fr 's3.marvideo.fr'; style-src 'self' 'unsafe-inline' https:; img-src 'self' data: https: blob:; font-src 'self' https: data:; connect-src 'self' https:;">
</head>

<%- include('./loader.ejs') %>

    <body>
        <div class="background"></div>
        <article id="profile-article">
        </article>
    </body>
    <script type="application/json" id="plinkk-ctx-username"><%- JSON.stringify(username || "") %></script>
    <script type="application/json"
        id="plinkk-ctx-identifier"><%- JSON.stringify((page && page.slug) ? page.slug : "") %></script>
    <script>
        (function () {
            try {
                const u = document.getElementById('plinkk-ctx-username')?.textContent || '';
                const s = document.getElementById('plinkk-ctx-identifier')?.textContent || '';
                window.__PLINKK_USERNAME__ = JSON.parse(u || '""');
                window.__PLINKK_IDENTIFIER__ = JSON.parse(s || '""');
            } catch (_) { }
        })();
        window.__PLINKK_EXPORT_MODE__ = <%= (typeof isExport !== 'undefined' && isExport) ? 'true' : 'false' %>;
        window.__PLINKK_IS_PREVIEW__ = (new URLSearchParams(location.search).get('preview') === '1');
        if (!window.__PLINKK_IDENTIFIER__) {
            try {
                const parts = location.pathname.split('/').filter(Boolean);
                if (parts[1] === '0') window.__PLINKK_IDENTIFIER__ = '';
            } catch (_) { }
        }
    </script>
    <script type="module"
        src="/<%= (typeof publicPath !== 'undefined' && publicPath) ? publicPath : ((page && page.slug) ? page.slug : username) %>.js"></script>
    <script type="module" src="https://unpkg.com/ionicons@7.1.0/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@7.1.0/dist/ionicons/ionicons.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/animejs/3.2.1/anime.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/gsap@3.9.1/dist/gsap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/simplex-noise/2.4.0/simplex-noise.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/dat-gui/0.7.7/dat.gui.min.js"></script>

</html>