{
  "scripts": {
    "prisma:migratedev": "prisma migrate dev",
    "prisma:dev": "prisma dev",
    "prisma:studio": "prisma studio",
    "prisma:init": "prisma migrate deploy && prisma generate --schema=../../prisma/schema.prisma",
    "postinstall": "prisma generate || true",
    "set-admin": "node scripts/set-admin.cjs",
    "migrate:plinkks": "node scripts/migrate-plinkks.cjs",
    "delete_inactive": "node scripts/delete_inactive_user.js ",
    "seed:role": "ts-node prisma/seed.ts",
    "prisma:generate": "prisma generate --generator=dashboard && prisma generate --generator=public",
    "build": "pnpm -r build"
  },
  "devDependencies": {
    "prisma": "^6.17.1"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1"
  },
  "private": true
}
